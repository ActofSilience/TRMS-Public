<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>


	<servlet>
		<servlet-name>MasterServlet</servlet-name>
		<display-name>MasterServlet</display-name>
		<description></description>
		<servlet-class>com.hagan.servlets.MasterServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>Display</servlet-name>
		<display-name>Display</display-name>
		<description></description>
		<servlet-class>com.hagan.servlets.Display</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>MasterServlet</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>Display</servlet-name>
		<url-pattern>/Display</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>login-index.html</welcome-file>
		<welcome-file>userlogin.html</welcome-file>
		<welcome-file>reimbursementform.html</welcome-file>
		<welcome-file>administration.html</welcome-file>
		<welcome-file>supervisormenu.html</welcome-file>
	
	</welcome-file-list>


</web-app>